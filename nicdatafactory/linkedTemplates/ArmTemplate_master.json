{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "nicdatafactory"
		},
		"AzureSynapseAnalytics1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSynapseAnalytics1'"
		},
		"datalakelinkedservice_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalakelinkedservice'"
		},
		"sql_db_linked_service_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'sql_db_linked_service'"
		},
		"vmlinkedservice_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'vmlinkedservice'"
		},
		"datalakelinkedservice_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://nicdatalakedemo.dfs.core.windows.net/"
		},
		"sql_db_linked_service_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "nicdemo.database.windows.net"
		},
		"sql_db_linked_service_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "appdb"
		},
		"sql_db_linked_service_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		},
		"vmlinkedservice_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\data"
		},
		"vmlinkedservice_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "shiruser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "nicdatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSynapseAnalytics1_connectionString": {
						"value": "[parameters('AzureSynapseAnalytics1_connectionString')]"
					},
					"datalakelinkedservice_accountKey": {
						"value": "[parameters('datalakelinkedservice_accountKey')]"
					},
					"sql_db_linked_service_password": {
						"value": "[parameters('sql_db_linked_service_password')]"
					},
					"vmlinkedservice_password": {
						"value": "[parameters('vmlinkedservice_password')]"
					},
					"datalakelinkedservice_properties_typeProperties_url": {
						"value": "[parameters('datalakelinkedservice_properties_typeProperties_url')]"
					},
					"sql_db_linked_service_properties_typeProperties_server": {
						"value": "[parameters('sql_db_linked_service_properties_typeProperties_server')]"
					},
					"sql_db_linked_service_properties_typeProperties_database": {
						"value": "[parameters('sql_db_linked_service_properties_typeProperties_database')]"
					},
					"sql_db_linked_service_properties_typeProperties_userName": {
						"value": "[parameters('sql_db_linked_service_properties_typeProperties_userName')]"
					},
					"vmlinkedservice_properties_typeProperties_host": {
						"value": "[parameters('vmlinkedservice_properties_typeProperties_host')]"
					},
					"vmlinkedservice_properties_typeProperties_userId": {
						"value": "[parameters('vmlinkedservice_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}